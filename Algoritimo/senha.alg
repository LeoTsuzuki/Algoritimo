algoritmo "semnome"
var
senha:inteiro
contador:inteiro
inicio
Para contador <- 1 ate 3 faca
     Escreval("Digite a senha:")
     leia(senha)
     se (senha <> 4321)entao
        Escreval("Senha errada número",contador)
     senao
        Escreval("SENHA CORRETA")
        interrompa
     fimse
fimpara
fimalgoritmo
